@page "/person"

<EntityForm TEntity="Person" Entity="person" RequestUri="api/persons" Title="Person">
    <TextField @bind-Value="person.Id" readonly />
    <TextField @bind-Value="person.Name" />
    <TextField @bind-Value="person.Email" />
    <DateField @bind-Value="person.BirthDate" />
</EntityForm>


@code {
    Person person = new Person() { Id = "1" };
}

